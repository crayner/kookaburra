{
    "name": "crayner/kookaburra",
    "description": "Symfony implementation of Gibbon",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "aura/sqlquery": "3.*-dev",
        "crayner/doctrine-bundle": "^1.0",
        "doctrine/annotations": "^1.7",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^2.1",
        "google/apiclient": "^2.2",
        "kookaburra/system-admin": "dev-master",
        "kookaburra/user-admin": "dev-master",
        "matthewbdaly/sms-client": "^1.0",
        "phpoffice/phpspreadsheet": "^1.8",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/asset": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/google-mailer": "4.3.*",
        "symfony/mailchimp-mailer": "4.3.*",
        "symfony/messenger": "4.3.*",
        "symfony/orm-pack": "^1.0",
        "symfony/property-info": "4.3.*",
        "symfony/security-bundle": "4.3.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "4.3.*",
        "symfony/validator": "4.3.*",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "4.3.*",
        "twig/cssinliner-extension": "^2.0"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/flex": "^1.4",
        "symfony/maker-bundle": "^1.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Gibbon\\": "Gibbon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "application:install --relative --symlink ": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "php -r \"copy('config/packages/kookaburra.yaml.dist', 'config/packages/kookaburra.yaml');\"",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
