{
    "name": "crayner/kookaburra",
    "description": "Symfony implementation of Gibbon",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "ext-calendar": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "aura/sqlquery": "3.*-dev",
        "crayner/ckeditor-bundle": "2.1.51",
        "crayner/doctrine-bundle": "^1.0",
        "doctrine/annotations": "^1.7",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "fpdf/fpdf": "^1.81",
        "google/apiclient": "^2.2",
        "kookaburra/activities": "dev-master",
        "kookaburra/departments": "dev-master",
        "kookaburra/library": "dev-master",
        "kookaburra/roll-groups": "dev-master",
        "kookaburra/school-admin": "dev-master",
        "kookaburra/system-admin": "dev-master",
        "kookaburra/user-admin": "dev-master",
        "matthewbdaly/sms-client": "^1.0",
        "phpmailer/phpmailer": "^6.1",
        "phpoffice/phpspreadsheet": "^1.8",
        "sensio/framework-extra-bundle": "^5.4",
        "symfony/asset": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.1",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/google-mailer": "^5.0",
        "symfony/mailer": "^5.0",
        "symfony/messenger": "^5.0",
        "symfony/orm-pack": "^1.0",
        "symfony/property-info": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/translation": "^5.0",
        "symfony/twig-bundle": "^5.0",
        "symfony/twig-pack": "dev-master",
        "symfony/validator": "^5.0",
        "symfony/webpack-encore-bundle": "^1.6",
        "symfony/yaml": "^5.0",
        "tecnickcom/tc-lib-barcode": "^1.15",
        "twig/cssinliner-extra": "3.x-dev"
    },
    "require-dev": {
        "symfony/debug-pack": "^1.0",
        "symfony/flex": "^1.4",
        "symfony/maker-bundle": "^1.11"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Gibbon\\": "Gibbon/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-curl": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "application:install --relative --symlink ": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "kookaburra:module:install": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    }
}
